<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="sst.bank.controllers.OperationsListController">
	<center>
		<TableView layoutX="0.0" layoutY="0.0"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<columns>
				<TableColumn fx:id="idColumn" prefWidth="75.0" text="Id" />
				<TableColumn fx:id="execDateColumn" prefWidth="75.0"
					text="Exec. Date" />
				<TableColumn fx:id="valueDateColumn" prefWidth="75.0"
					text="Value Date" />
				<TableColumn fx:id="amountColumn" prefWidth="75.0" text="Amount" />
				<TableColumn fx:id="counterpartyColumn" prefWidth="75.0"
					text="Counterparty" />
				<TableColumn fx:id="detailsColumn" prefWidth="75.0"
					text="Details" />
			</columns>
		</TableView>
	</center>
</BorderPane>
